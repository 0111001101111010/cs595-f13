var fsize = 1.0;

function readCookie(){
	if (document.cookie.length>0){ 
		var myregex = /fontSize=(\d\.\d)/;
		var mycookie = document.cookie.match(myregex);
		if (mycookie==null || mycookie[1]==null){
			return -1;
		}
		return mycookie[1];
	}else{
		return -1;
	}
}

function setFontSize(){
	var myCookie = readCookie();
	if(myCookie == null || myCookie == -1){
		return;
	}
	var myArticle = document.getElementById("article");
	fsize = parseFloat(myCookie);
	myArticle.style.fontSize = fsize + "em";
}

function changeFontsize(increment) {
	//alert("Inside the function");
	var myArticle = document.getElementById("article");
	var fontIncrease;
	var n=(location.href).split("."); 
	var t=n.indexOf("litinsider");
	//alert(n[t]);
	if(n[t]=="litinsider")
	{
	if(fsize == 1.0)
	{
	fsize = 0.625;
	}
	if (increment != "") {
	  fontIncrease = parseInt(increment);
	  if(fontIncrease == 1){
	  	if(fsize < 1.425){
	  		fsize = fsize + 0.1;
	  	}
	  }else{
	  	if(fsize > 0.525){
		  	fsize = fsize - 0.1;
		}
	  }
	}else{
		if(fsize < 1.425){
			fsize = fsize + 0.1;
		}
	}
	}
	else
	{
	if (increment != "") {
	  fontIncrease = parseInt(increment);
	  if(fontIncrease == 1){
	  	if(fsize < 1.8){
	  		fsize = fsize + 0.1;
	  	}
	  }else{
	  	if(fsize > 0.9){
		  	fsize = fsize - 0.1;
		}
	  }
	}else{
		if(fsize < 1.8){
			fsize = fsize + 0.1;
		}
	}
	
	}
	myArticle.style.fontSize = fsize + "em";
	document.cookie = 'fontSize=' + fsize;
}//function


</script>
