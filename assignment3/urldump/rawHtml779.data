		});

		$("#dialog-guar").dialog({
			autoOpen: false,
			width: 550
		});

		$('.cfheader .offer-blocks ul li.lpg').click(function() {
			$("#dialog-guar").dialog( "option", "title", "<div class='hblock'>Lowest Price Guaranteed</div>" );
			$("#dialog-guar").dialog("open");
		});

		/* hover states */
		$(".cfheader .offer-blocks ul li.delivery").hover(function(){
			$(this).toggleClass("delivery-hover");
		});

		$(".cfheader .offer-blocks ul li.dress").hover(function(){
			$(this).toggleClass("dress-hover");
		});

		$(".cfheader .offer-blocks ul li.ls").hover(function(){
			$(this).toggleClass("ls-hover");
		});

		$(".cfheader .offer-blocks ul li.lpg").hover(function(){
			$(this).toggleClass("lpg-hover");
		});

		/* ==== CF FINISH ====*/


		$("#footer_desc_show").hide();
		$("#footer_desc_hide").show();

		$("#footer_desc_holder").mouseover(function() {
			$("#footer_desc_show").show();
			$("#footer_desc_hide").hide();
		});

		$("#footer_desc_holder").mouseout(function() {
			$("#footer_desc_show").hide();
			$("#footer_desc_hide").show();
		});

		$("#header_search_text").focus(function() {
			$(this).val("");
		});

		$("#header_search_text").blur(function() {
			if ($(this).val()=="") {
				$(this).val("What are you looking for?");
			};
		});

		$(window).resize(function() {
			var iOffset=$("#page_holder").offset();

			if (iOffset.left < 100) {
				// Hide toolbar when no margin exists
				$("#like_bar").stop();
				$("#like_bar").animate({
					"top": ($(window).scrollTop() + 100) +"px",
					"left": "-100px"
				},"slow");

				$(window).unbind("scroll");
			} else {
				// Show toolbar
				$("#like_bar").stop();
				$("#like_bar").animate({
					"top": ($(window).scrollTop() + 100) +"px",
					"left": (iOffset.left - 85) +"px"
				},"slow");

				$(window).scroll(function (e) {
					var iOffset=$("#page_holder").offset();
					$("#like_bar").stop();
					$("#like_bar").animate({
						"top": ($(window).scrollTop() + 100) +"px",
						"left": (iOffset.left - 85) +"px"
					},"slow");
				});
			}
		});

		// Fire the trigger
		$(window).resize();
	});
</script>

