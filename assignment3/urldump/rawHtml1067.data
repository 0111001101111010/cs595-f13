	function uniqueAddition()
	{// return almost unique string from current date/time: Two letters from month + date + . + hours + minutes + seconds + . + milliseconds + random number between 1 and 1000:
	 // Example SE11.234501.971
		var dtToday = new Date();
		var ret = "";
		ret += arrMonths[dtToday.getMonth()];
		ret += dtToday.getDate();
		ret += "." + dtToday.getHours();
		if(dtToday.getMinutes() < 10) ret += "0"
		ret += dtToday.getMinutes();
		if(dtToday.getSeconds() < 10) ret += "0"
		ret += dtToday.getSeconds();
		ret += "." + dtToday.getMilliseconds();
		ret += "." + Math.floor((Math.random()*1000)+1); 
		return ret;
	}

	function singleRplc(sourceString,regEx,newString)
	{
		var arrExec = regEx.exec(sourceString);
		if(arrExec)
			sourceString = sourceString.substring(0,regEx.lastIndex-arrExec[0].length) + newString + sourceString.substring(regEx.lastIndex);
		return sourceString;
	}
	function addTitleStartWords(title)
	{
		var s = "";
		if(isHardDriveSelected())
			s = "Custom Desktop";
		else 
			s = "Custom Barebone";
		s += " " + uniqueAddition() + " ";
		return s + title;
	}
	function isHardDriveSelected()
	{
		if(!/NOT *SE?LECTED/ig.test(document.system.option_8.options[document.system.option_8.selectedIndex].text)) return true; if(!/NOT *SE?LECTED/ig.test(document.system.option_9.options[document.system.option_9.selectedIndex].text)) return true; 
		return false;
	}

	function setTitle() //draftTitle = document.system.options[document.system.selectedIndex].text
