        <h2>Register!</h2>
        <div>
            <span>In order to register to this conference you need to create an account on this site (so you can manage your registration details later on).</span><br /><br /><span>
                <a href="http://2013.ploneconf.org/login" id="login_form">
                If you already have an account, click here to login.
                </a>
            </span><br /><span>
                <a href="http://2013.ploneconf.org/@@register" id="register_form">
                Click here to create an account.
                </a>
            </span><br /><br /><script>
            jQuery(function(){
                var common_content_filter = '#content>*:not(div.configlet),dl.portalMessage.error,dl.portalMessage.info';
                // login form
                jQuery('#login_form').prepOverlay(
                    {
                        subtype: 'ajax',
                        filter: common_content_filter,
                        formselector: 'form#login_form',
                        noform: function () {
                            if (location.href.search(/pwreset_finish$/) >= 0) {
                                return 'redirect';
                            } else {
                                return 'reload';
                            }
                        },
                        redirect: function () {
                            var href = location.href;
                            if (href.search(/pwreset_finish$/) >= 0) {
                                return href.slice(0, href.length-14) + 'logged_in';
                            } else {
                                return href;
                            }
                        }
                    }
                );
                jQuery('#register_form').prepOverlay(
                    {
                        subtype: 'ajax',
                        filter: common_content_filter,
                        formselector: 'form.kssattr-formname-register'
                    }
                );
            });
            </script></div>
