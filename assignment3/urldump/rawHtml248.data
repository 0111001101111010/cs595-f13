			var item   = $( this );
			var row	   = item.parents( 'li.com' );

			// Remove button, show loading
			item.loading();

			$.ajax( {
				url: item.attr( 'href' ) + '&ajax=true',
				success: function( response ) {
					if ( PollDaddy.is_fatal( response ) )
						item.loading();
					else {
						row.replaceWith( response );
						comment_init();
					}
				}
			} );
			return false;
		});
