       <script type="text/javascript">
        $(function() {

          $('.feedback').click(function(){
            $('#modal-feedback').modal();
          });

          var form_selector = "#feedback_form form";

          function on_form_submitted(evt) {
            evt.preventDefault();

            var $form = $(this);
            $.post(
              $form.attr("action"),
              $form.serialize(),
              function(response) {
                var $newform = $("form", response);
                var ok = $(".error, .error_log", $newform).length ? false : true;
                if (ok) {
                  $form.replaceWith($("<ul class='more-user-messages'><li class='success'>Votre message a été envoyé</li></ul>"));
                }else {
                  $form.replaceWith($newform);
                  $(form_selector).submit(on_form_submitted);
                }
              }
            );
            return false;
          };
          $(form_selector).submit(on_form_submitted);
        });
      </script>


