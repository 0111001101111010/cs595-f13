    var modal_shown;
    var show_registration = function (evt) {
        evt.preventDefault();
        modal_shown = "registration";
        $("#modal").one('show.bs.modal', function() {
            $(window).trigger('registration-switch');
        });

        $('#modal').modal({
          remote: "/myblookup/register/",
        })
    };
    $(document).ready(function () {
      $("#login-link").on('click', function(evt) {
        evt.preventDefault();
        modal_shown = "login";
        $("#modal").one('shown.bs.modal', function(evt) {
          $(window).trigger('login-switch');
        });
        $('#modal').modal({
          remote: "/myblookup/register/",
        })
      })
      $("#registration-link").on('click', show_registration);
      
        $("#create-blook-link").on('click', show_registration);
      
    });
  </script>

