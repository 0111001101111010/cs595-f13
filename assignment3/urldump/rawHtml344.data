<script id="tmpl-submit-challenge-app-popup" type="text/x-jquery-tmpl">
    <section class="submit-challenge-app-cont">
        <header>
            <h2 class="multiline">Choose an app to submit</h2>
        </header>
		
        <div data-bind="visible:IsLoading">
            Loading Apps...
        </div>
		
        <div data-bind="visible:IsSubmitting">
            Saving...
        </div>

        <div data-bind="visible:!IsLoading() && !IsSubmitting()">

            <p data-bind="visible:!(Apps().length)">No available apps were found.</p>

            <ul data-bind="foreach:Apps" class="sans hor-list apps-list">
                <li>
                    <div class="app-widget" data-bind="attr:{'data-app-id':$data.StoreId}, click:$parent.HandleAppClick, css:{selected:($parent.SelectedApp()==$data)}">
                        <img data-bind="attr:{'src':$data.ThumbUrl}" class="app-img" />
                        <span class="app-name" data-bind="text:$data.Title"></span>
					</div>
				</li>
			</ul>
		</div>
			
		<footer class="close-overlay-cont">
			<a class="close-overlay btn cancel" data-bind="click:$parent.Close">Cancel</a>
			<a class="close-overlay btn" data-bind="click:Submit">Submit</a>
		</footer>
	</section>
</script>
