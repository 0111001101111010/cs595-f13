<script src="/Scripts/jquery.validate.unobtrusive.js"></script>

        <script type="text/javascript">
            $(function () {

                $('input').placeholder();

                var data = $("meta[name='accept-language']").attr("content");
                globalization = Globalize.culture(data);
                //parseFloat = Globalize.parseFloat;
                //$.global.preferCulture(data);

                var language = data.split('-')[0];
                $.datepicker.setDefaults($.datepicker.regional[language]);

                $.validator.methods.number = function (value, element) {
                    return this.optional(element) || !isNaN(globalization.parseFloat(value));
                };

                $.validator.methods.date = function (value, element) {
                    return this.optional(element) || Globalize.parseDate(value) != null;
                };

                $('#wzpr-footer a').hover(function () {
                    //alert($('#wzpr-footer div').not($(this)).length);
                    $('#wzpr-footer a').not($(this)).css('opacity', 0.2);
                    $(this).css('opacity', 0.71);
                }, function () {
                    if ($(this).hasClass('active')) {
                        $(this).css('opacity', 1);
                    }
                    else {
                        $(this).css('opacity', 0.71);
                    }
                });

                $('#wzpr-footer').bind('mouseleave', function () {
                    $('#wzpr-footer a').css('opacity', 0.2);
                    $('#wzpr-footer a.active').css('opacity', 1);
                });
            });


        </script>

        


