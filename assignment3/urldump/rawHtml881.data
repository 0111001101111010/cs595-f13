		        }
		        elem = elems[i]; //The only, or the best iFrame
		        if (elem.id) return elem.id; //Existing ID, return it
		        // else: Create a new ID
		        do { //Keep postfixing `-frame` until the ID is unique
		            id += "-frame";
		        } while (document.getElementById(id));
