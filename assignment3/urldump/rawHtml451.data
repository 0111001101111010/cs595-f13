                });

                $('#wdk_slide-container').hover(function() {
                    $('#wdk_shop_menu').clearQueue();
                    $('#wdk_shop_menu ul.wdk-shopCatList:first').show();
                    $('#wdk_shop_a').addClass('down_arrow');
                    $('#wdk_shop_menu').show();
                }, function() {
                    $('#wdk_shop_menu').stop(true, true).delay(400).slideUp(400);
                    $('#wdk_shop_a').removeClass('down_arrow');
                });
       
                $('.wdk-shopCatList li').hover(
                function() {
                    $(this).find('ul.wdk-shopCatList:first').show();
                },
                function(){
                    $(this).find('ul.wdk-shopCatList:first').hide();
                });

                // Javascript for DDMs
                $('.wdk_level_1 > a span').each(function(){
                    if( $(this).text().length > 18 ){
