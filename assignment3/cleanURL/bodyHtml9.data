&lt;script src="http://www.10news.com/sps-kgtv-theme/js/main.js?browserId=other&amp;amp;minifierType=js&amp;amp;languageId=en_US&amp;amp;b=6110&amp;amp;t=1380328433000" type="text/javascript"&gt;&lt;/script&gt;



	

	

	&lt;script type="text/javascript"&gt;
