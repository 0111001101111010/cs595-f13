    <script type="text/javascript">
    
        var loadScriptsOnDomReady = new Array();
        var executeScriptsOnDomReady = new Array();
        var loadIFramesOnDomReady= new Array();
        
        function queueAsyncIFrame(width, height, style, targetId, url) {
            params = new Array()
            params[0] = width;
            params[1] = height;
            params[2] = style;
            params[3] = targetId;
            params[4] = url;
            loadIFramesOnDomReady.push(params);
            }
        
        function pageInit() {
            for(count = 0; count < loadScriptsOnDomReady.length; count++) {
                var resource = document.createElement('script'); 
                resource.src = loadScriptsOnDomReady[count];
                var script = document.getElementsByTagName('script')[0];
                script.parentNode.insertBefore(resource, script);
                }
            
            for(count = 0; count < loadIFramesOnDomReady.length; count++) {
                var iframe = document.createElement('iframe');
                iframe.setAttribute("frameborder", "0");
                // Required only by IE6, of course
                iframe.setAttribute("frameBorder", "0");
                iframe.setAttribute("marginheight",  "0"); 
                iframe.setAttribute("marginwidth", "0"); 
                iframe.setAttribute("scrolling", "no");
                iframe.setAttribute("width", loadIFramesOnDomReady[count][0]);
                iframe.setAttribute("height", loadIFramesOnDomReady[count][1]);
                iframe.setAttribute("style", loadIFramesOnDomReady[count][2]);
                document.getElementById(loadIFramesOnDomReady[count][3]).appendChild(iframe);
                iframe.src = loadIFramesOnDomReady[count][4]
                }

            for(count = 0; count < executeScriptsOnDomReady.length; count++) {
                (new Function(executeScriptsOnDomReady[count]))();
                }
            
            }

        // Check for browser support of event handling capability
        if (window.addEventListener)
            window.addEventListener("load", pageInit, false);
        else if (window.attachEvent)
            window.attachEvent("onload", pageInit);
            else 
                window.onload = pageInit;
    </script>
    
