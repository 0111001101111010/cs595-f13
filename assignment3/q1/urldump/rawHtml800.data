<script type="text/javascript">


function closeModal() {
    $("#account-workflow").dialog("close");
    //show all media/flash players, if no reload of page
    $('object, embed, .multimedia, .ad').show();
}

function updateModalTitle(text){
    $("#account-workflow").dialog("option", "title", $.trim($('<div>').html(text).text()) );
}
var close_redirect = false;
var close_redirect_url = false;

var acmgt_dialog_options = {
    autoOpen: false,
    resizable: false,
    draggable: false,
    closeOnEscape: true,
    width: '576',
    modal: true,
    title: "",
    zIndex: 2000000,
    open: function( event, ui ) {
        //hide all media/flash players because of overlay issues
        $('object, embed, .multimedia, .ad').hide();

        // only for safari/chrome browsers we unbind some default settings used by jquery.modal to allow us to use the scroll bars
        if ( $.browser.safari ) {
            window.setTimeout(function() {
                jQuery(document).unbind('mousedown.dialog-overlay')
                                .unbind('mouseup.dialog-overlay');
            }, 100);
        }
    }
};
var acmgt_dialog_close_hash = {
    close: function( event, ui ) { 
        $('#modal-iframe').attr('src', 'about:blank');
        if ( close_redirect && close_redirect_url ) {
            window.location = close_redirect_url;
        } else if ( window.location.protocol == "https:" ) {
            window.location = "http://" + window.location.hostname + window.location.pathname;
        } else {
            window.location = document.location.pathname;
        }
    }
};

var acmgt_dialog_close_remove_hash = {
    close: function( event, ui ) {
        window.location = document.location.pathname;
    }
};

var acmgt_dialog_close = {
    close: function( event, ui ) { 
        $('#modal-iframe').attr('src', 'about:blank');
        var ua = $.browser;
        if ( ua.mozilla && ua.version.slice(0,3) == "1.9" ) {
            window.location.href = document.location.href;
        } else {
            window.location.reload();
        }
    }
};

var show_close = true;

$().ready(function() {
    
    $('a[href^="/subscriptions/subscribe"]').click(function (e) {
        // check to see current page is not "subscriptions"
        var pattern = /^\/subscriptions/;
        if( ! pattern.test(window.location.pathname) ) {
            e.preventDefault();
            
            // check to see if there are any params on goto url
            var qpattern  = /\?/;
            var q = ( qpattern.test( $(this).attr('href') ) ) ? '&' : '?';
            window.location = $(this).attr('href') + q +'cf=' + window.location.pathname;
        }
    });

    $('.account-trigger').click(function(e) {
        e.preventDefault();
        var mTitle = ($(this).attr('title')) ? $(this).attr('title') : 'My Account';
        //forums
        if ( $(this).hasClass('url-redirect') ){
            openAccountWorkflow('/accounts/usernameautogen?r=' + $(this).attr('href'), 'Create a user name', acmgt_dialog_close, show_close); 
        } else {
            var url = $(this).attr('url');
            // switch to ssl
            if ( window.location.protocol == "http:" && url.match(/^\/subscriptions/i) ) {
                var redirurl = "https://" + window.location.hostname + window.location.pathname + "#!" + url;
                window.location = redirurl;
                return false;
            } 
            openAccountWorkflow(url, mTitle, acmgt_dialog_close, show_close);
        }
        return false;
    });

    var urlHash = window.location.hash;
    if ( urlHash ) {
        var path = urlHash.substring(2);
        if ( path.match(/^\/accounts/i) ) {
            var title = "";
            var verifyType = urlHash.split('/');
            switch(verifyType[3]){
                case 'cp':
                    title = "Create a free account";
                    // hide close button in upper right corner
                    show_close = false;
                    break;
                case 'ce':
                    title = "Edit your e-mail address";
                    close_redirect = true;
                    break;
                case 'n':
                    title = "Newsletter sign-up";
                    break;
                case 'e':
                    title = "Log in";
                    close_redirect = true;
                    break;
                case 's':
                    title = "Subscribe to <em>The Chronicle</em>";
                    show_close = false;
                    break;
            }
            if ( path.match(/^\/accounts\/signupnewsletter/) ) {
                title = "Newsletter sign-up";
            }
            openAccountWorkflow(path, title, acmgt_dialog_close_hash, show_close);
        } else if ( path.match(/^\/subscriptions/i) ) {
            title = "Subscribe to <em>The Chronicle</em>";
            openAccountWorkflow(path, title, acmgt_dialog_close_hash, show_close);
        }
    }

    $('#close').click(function() {
        closeModal();
    });

});

function openAccountWorkflow(href, mTitle, options, show_close) {
    /* merge defaults and options, without modifying defaults */
    var settings = $.extend({}, acmgt_dialog_options, options, {title: mTitle});    
    var ua = $.browser;
    $("#account-workflow").dialog(settings);        
    if ( ua.mozilla && (ua.version.slice(0,3) <= "1.9") && (document.readyState != "complete") ) {
        // Firefox 3.6 and under has a bug setting src of iframe so wait till page is done loading to set
        $(window).load(function() {
            $('#modal-iframe').attr('src', href);
        });
    } else {
        // all other browsers set right away.
        $('#modal-iframe').attr('src', href);
    }
    $("#account-workflow").dialog("open");
    if ( !show_close ) {
        $('a.ui-dialog-titlebar-close').hide();
    }
}
function acWorkFlowShowClose() {
    $('a.ui-dialog-titlebar-close').show();
}

</script><!-- Begin: Eloqua -->
