    <script type="text/javascript">

    window.STOP_APP = true;

    function getCookieSrsid()
    {
        var match = document.cookie.match(/SRSID=(\w+)/);
        return match ? match[1] : '';
    }

    function srsid()
    {
        var oldId = getCookieSrsid();

        var img = new Image();
        img.onload = function()
        {
            var newId = getCookieSrsid();

                        if (newId == oldId) {
                window.STOP_APP = false;
                if (window.UTIL)
                    UTIL.init();
            } else {
                refreshPage();
            }
        };
        img.src = '/checksrsid';
    }
    </script>

